<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>masterserver</servlet-name>
    <servlet-class>org.freshworks.MasterSlaveFileReplication.MasterServer</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>masterserverlsm</servlet-name>
    <servlet-class>org.freshworks.MasterSlaveFileReplication.MasterSlaveLSM</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>masterserver</servlet-name>
    <url-pattern>/masterserver</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>masterserverlsm</servlet-name>
    <url-pattern>/masterserverlsm</url-pattern>
  </servlet-mapping>

</web-app>
